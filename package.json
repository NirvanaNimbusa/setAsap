{
  "name": "setasap",
  "version": "1.1.0",
  "description": "setImmediate polyfill for the browser and node",
  "main": "setAsap.js",
  "scripts": {
    "test": "karma start && ./node_modules/.bin/mocha test/*",
    "build": "grunt build"
  },
  "repository": {
    "type": "git",
    "url": "https://taylorhakes@github.com/taylorhakes/setAsap.git"
  },
  "keywords": [
    "setImmediate",
    "task",
    "setAsap",
    "setTimeout",
    "process.nextTick"
  ],
  "testling": {
    "harness": "mocha-bdd",
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "chrome/latest",
      "firefox/latest"
    ]
  },
  "author": "Taylor Hakes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taylorhakes/setAsap/issues"
  },
  "homepage": "https://github.com/taylorhakes/setAsap",
  "devDependencies": {
    "browserify": "^13.0.0",
    "chai": "^1.9.1",
    "grunt": "^0.4.4",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-uglify": "^0.4.0",
    "karma": "^0.12.12",
    "karma-browserify": "^5.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.3",
    "karma-mocha": "^0.1.3",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-sauce-launcher": "^0.2.7",
    "mocha": "^1.18.2",
    "watchify": "^3.7.0"
  }
}
